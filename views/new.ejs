<% include partials/header %>

<div class="container">
	<div clas="row">
		<h1 id="add-header">Add an art</h1>
			<div id="add-form">
				<form action="/art" method="POST">
					<div class="form-group">
						<input class="form-control" type="text" name="title" placeholder="Art Title">
					</div>
					<div class="form-group">
						<input class="form-control" type="text" name="image" placeholder="Image URL">
					</div>
					<div  clas="form-group">
						<input class="form-control" type="text" name="description" placeholder="Description">
					</div>
					<div class="form-group">
						<button id="add-button" class="btn btn-lg btn-primary btn-block">Submit</button>
					</div>
				</form>
			</div>
	</div>
</div>

<% include partials/footer %>