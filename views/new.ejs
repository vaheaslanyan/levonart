<% include partials/header %>

<div class="container">
			<h3>Add new art</h3>
			<div id="add-form">
				<form action="/art" method="POST">
					<div class="form-group">
						<label>Art Title</label>
						<input class="form-control" type="text" name="title" placeholder="Title of the art">
					</div>
					<div class="form-group">
						<label>Image URL</label>
						<input class="form-control" type="text" name="image" placeholder="Image URL address">
					</div>
					<div  class="form-group">
						<label>Description</label>
						<input class="form-control" type="text" name="description" placeholder="Art description">
					</div>
					<div  class="form-group">
						<label>ArtPal URL</label>
						<input class="form-control" type="text" name="artUrl" placeholder="Print purchase URL address">
					</div>
					<div class="form-group">
						<button id="add-button" class="btn btn-lg btn-primary btn-block">Submit</button>
					</div>
				</form>
			</div>
</div>

<% include partials/footer %>